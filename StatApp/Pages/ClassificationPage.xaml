<Page x:Class="StatApp.Pages.ClassificationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StatApp.Controles"
      mc:Ignorable="d" 
     
	Title="Classification">
    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <DockPanel>
            <local:MainMenuUserControl DockPanel.Dock="Top" Margin="5,5"  HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <local:ClusterizeUserControl DataContext="{Binding Path=OrdModelView}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="5,5" />
        </DockPanel>
    </ScrollViewer>
</Page>
